@model IEnumerable<webapiconsumemvc.Models.Productviewmodel>

@{
    ViewBag.Title = "Getmembers";
}

<h2>Getmembers</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.productno)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.pname)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.pdetail)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.productno)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.pname)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.pdetail)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id = item.productno }) |
            @Html.ActionLink("Details", "Details", new { id = item.productno }) |
            @Html.ActionLink("Delete", "Delete", new { id = item.productno })
        </td>
    </tr>
}

</table>
